meta {
  name: Specific Bundle Object
  type: http
  seq: 2
  tags: [
    authenticated
  ]
}

post {
  url: {{baseUrl}}/mdsammler/remove/bundleobject?anbieter={{anbieter}}&appname={{appname}}&appversion={{appversion}}&schema-id={{dynamic_schema_id}}&object-uuid={{object-uuid}}
  body: none
  auth: bearer
}

params:query {
  anbieter: {{anbieter}}
  appname: {{appname}}
  appversion: {{appversion}}
  schema-id: {{dynamic_schema_id}}
  object-uuid: {{object-uuid}}
}

headers {
  Content-Type: application/json
}

auth:bearer {
  token: {{dynamic_jwt}}
}
