meta {
  name: Validated Bundle
  type: http
  seq: 3
  tags: [
    authenticated
  ]
}

post {
  url: {{baseUrl}}/admin/app/remove/validatedbundle?anbieter={{anbieter}}&appname={{appname}}&appversion={{appversion}}&schema-id={{dynamic_schema_id}}
  body: none
  auth: bearer
}

params:query {
  anbieter: {{anbieter}}
  appname: {{appname}}
  appversion: {{appversion}}
  schema-id: {{dynamic_schema_id}}
}

auth:bearer {
  token: {{dynamic_jwt}}
}

assert {
  res.status: neq 401
  res.status: eq 200
  res.status: neq 404
}
