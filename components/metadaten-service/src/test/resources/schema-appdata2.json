{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "masterData": {
      "type": "object",
      "properties": {
        "anbieter": {
          "type": "string"
        },
        "appName": {
          "type": "string"
        },
        "appVersion": {
          "type": "string"
        }
      },
      "required": [
        "anbieter",
        "appName",
        "appVersion"
      ]
    },
    "metaData": {
      "type": "object",
      "properties": {
        "discoveryObject": {
          "type": "object",
          "properties": {
            "discoveryDetails": {
              "type": "object",
              "properties": {
                "detail1": {
                  "type": "string"
                },
                "detail2": {
                  "type": "string"
                },
                "detail3": {
                  "type": "string"
                },
                "sensationaldetail": {
                  "type": "string"
                }
              },
              "required": [
                "detail1",
                "detail2",
                "detail3",
                "sensationaldetail"
              ]
            },
            "name": {
              "type": "string"
            },
            "uuid": {
              "type": "string"
            }
          },
          "required": [
            "discoveryDetails",
            "name",
            "uuid"
          ]
        }
      },
      "required": [
        "discoveryObject"
      ]
    }
  },
  "required": [
    "masterData",
    "metaData"
  ]
}